<h2>Friends</h2>
<ul>
  <% if @friends.length > 0 %>
    <% @friends.each do |friend| %>
      <li><%= "#{friend['firstName']} #{friend['lastName']}" %></li>
    <% end %>
  <% else %>
    <%= image_tag('http://www.quickmeme.com/img/f5/f5ae1b8b2980d64de22ecee616d1277a73d35c21833e33b7e5646b5cbf84463f.jpg') %>
  <% end %>
</ul>
